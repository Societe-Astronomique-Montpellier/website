{
  "format": "page",
  "id": "event",
  "label": "Evenement",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Titre",
          "placeholder": ""
        }
      },
      "subtitle": {
        "type": "Text",
        "config": {
          "label": "Sous-titre",
          "placeholder": ""
        }
      },
      "send_notification": {
        "type": "Boolean",
        "config": {
          "label": "Notification à la sam-liste",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": false
        }
      },
      "image_vignette": {
        "type": "Image",
        "config": {
          "label": "Vignette",
          "constraint": {},
          "thumbnails": [
            {
              "name": "vignette",
              "width": 780,
              "height": 520
            },
            {
              "name": "mobile",
              "width": 320,
              "height": 212
            }
          ]
        }
      },
      "image_banner": {
        "type": "Image",
        "config": {
          "label": "Bannière image",
          "constraint": {},
          "thumbnails": [
            {
              "name": "banner",
              "width": 1280,
              "height": 450
            },
            {
              "name": "mobile",
              "width": 640,
              "height": 225
            }
          ]
        }
      },
      "access_type": {
        "type": "Select",
        "config": {
          "label": "Type d'accès",
          "placeholder": "Type d'accès à l'évenement",
          "options": [
            "Évenement public (gratuit)",
            "Évenement privé ou public avec inscription (payant)",
            "Réservé aux membres",
            "Évenement astronomique"
          ],
          "default_value": "Évenement public (gratuit)"
        }
      },
      "activite_periodique": {
        "type": "Boolean",
        "config": {
          "label": "Afficher dans activité périodique",
          "placeholder_false": "Masquer",
          "placeholder_true": "Afficher",
          "default_value": true
        }
      },
      "resume": {
        "type": "StructuredText",
        "config": {
          "label": "Contenu",
          "placeholder": "Contenu",
          "allowTargetBlank": true,
          "multi": "paragraph,heading4,heading5,heading6,strong,em,hyperlink,list-item,o-list-item,rtl,image"
        }
      },
      "carrousel": {
        "type": "Boolean",
        "config": {
          "label": "Images carrousel",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": false
        }
      },
      "time_start": {
        "type": "Timestamp",
        "config": {
          "label": "Date & Heure de début",
          "placeholder": "Date & Heure de début"
        }
      },
      "time_end": {
        "type": "Timestamp",
        "config": {
          "label": "Date & Heure de fin",
          "placeholder": "Date & Heure de fin"
        }
      },
      "place_event_txt": {
        "type": "Text",
        "config": {
          "label": "Lieu de l'évenement",
          "placeholder": ""
        }
      },
      "place_event": {
        "type": "GeoPoint",
        "config": {
          "label": "Coordonnées géographiques"
        }
      },
      "link": {
        "type": "Link",
        "config": {
          "label": "Lien",
          "placeholder": "Lien vers une page",
          "allowTargetBlank": false,
          "select": null
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "type": "Image",
        "config": {
          "label": "Meta image",
          "constraint": {},
          "thumbnails": []
        }
      }
    }
  }
}
